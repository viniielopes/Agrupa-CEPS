  "version": "0.2.0",
  "configurations": [
    {
      "type": "chrome",
      "request": "launch",
      "name": "TSX DEBUG",
      "url": "http://localhost:3000",
      "skipFiles": [
        "${workspaceFolder}/node_modules/**/*.js",
        "${workspaceFolder}/lib/**/*.js",
        "<node_internals>/**/*.js",
        "node_modules",
        "node_modules/**/*.js",
        "bundle.js"
      ],
      "webRoot": "${workspaceFolder}"
    }
  ]
}
